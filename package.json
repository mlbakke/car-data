{
  "name": "car-data",
  "version": "1.0.0",
  "description": "Landing page for a online store soon to launch",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlbakke/car-data.git"
  },
  "author": "Martin Liland Bakke",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mlbakke/car-data/issues"
  },
  "homepage": "https://github.com/mlbakke/car-data#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1",
    "postcss-cli": "^7.1.0"
  },
  "keywords": [
    "car",
    "specifications",
    "data",
    "auto"
  ]
}
